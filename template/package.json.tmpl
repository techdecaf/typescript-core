{
  "name": "{{ToLower .Name}}",
  "version": "0.0.1",
  "description": "{{.Description}}",
  "author": "{{.Author}}",
  "license": "UNLICENSED",
  "keywords": [],
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "types": "build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "{{.GIT_BASE_URL}}/{{.Name}}"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "ci": "yarn lint && yarn test:coverage && test:e2e",
    "lint": "eslint \"**/*.ts\" --fix --ignore-path .gitignore",
    "test": "yarn lint && jest",
    "test:coverage": "jest --coverage --globals '{}'",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch",
    "test:e2e": "echo 'no e2e tests configured'"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/experimental-utils": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "2.22.0",
    "husky": "^4.3.0",
    "jest": "^26.0.1",
    "jest-junit": "^11.0.1",
    "js-yaml": "^3.14.0",
    "ts-jest": "^26.1.0",
    "ts-node": "^9.0.0",
    "tsc-watch": "^4.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}
